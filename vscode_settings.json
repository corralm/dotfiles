{
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.guides.indentation": false,
    "editor.inlayHints.enabled": "off",
    "editor.minimap.enabled": false,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestionsDelay": 1500,
    "editor.suggest.showConstructors": false,
    "editor.suggest.showDeprecated": false,
    "editor.suggest.showEnumMembers": false,
    "editor.suggest.showEnums": false,
    "editor.suggest.showEvents": false,
    "editor.suggest.showReferences": false,
    "editor.suggest.showStructs": false,
    "editor.suggest.showUnits": false,
    "editor.suggest.showInterfaces": false,
    "editor.suggest.showOperators": false,
    "editor.tabCompletion": "on",
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.confirmSync": false,
    "jupyter.experiments.enabled": false,
    "notebook.breadcrumbs.showCodeCells": false,
    "notebook.consolidatedRunButton": true,
    "notebook.insertToolbarLocation": "hidden",
    "notebook.showCellStatusBar": "hidden",
    "notebook.output.wordWrap": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.condaPath": "/opt/miniconda3/envs/env",
    "python.defaultInterpreterPath": "/opt/miniconda3/envs/env/bin/python",
    "window.nativeTabs": true,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "JupyterLab Light",
    "workbench.iconTheme": "material-icon-theme",
}
